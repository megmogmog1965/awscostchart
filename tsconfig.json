{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "outDir": "./build",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "sourceMap": false
  },
  "buildOnSave": true,
  "filesGlob": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/typings/**/*.ts",
    "./src/typings/**/*.tsx",
    "!./node_modules/**/*.ts",
    "!./node_modules/**/*.tsx"
  ],
  "files": [
  ],
  "exclude": [

  ]
}
